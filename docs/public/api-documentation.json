  "paths": {
      "/u/auth/login":{
          "post": {
              "tags": ["Auth"],
              "summary": "User Email Login",
              "consumes": "application/json",
              "produces": "application/json",
              "requestBody": {
                  "content": {
                      "application/json": {
                          "type": "object",
                          "schema": {
                              "type": "object",
                              "properties": {
                                  "email": {
                                      "type": "string"
                                  },
                                  "password":{
                                      "type": "string"
                                  }
                              }
                          }
                      }
                  }
              },
              "responses": {
                  "200": {
                      "description": "operation successful",
                      "content":{
                          "application/json": {
                              "schema": {
                                  "type": "object",
                                  "properties": {
                                      "status": {"type": "boolean"},
                                      "type": { "type": "string", "description": "error or success"},
                                      "data": {
                                          "type": "object",
                                          "properties": {
                                              "token": {"type": "string"},
                                              "user": {
                                                  "$ref": "#/definitions/UserResponseObj"
                                              }
                                          }
                                      },
                                      "message": {"type" :"string"}
                                  }
                              }
                          }
                      }
                  },
                  "400": {
                      "description": "client error"
                  },
                  "500":{
                      "description": "server error"
                  }
              }
          }
      },
      "/u/auth/register": {
