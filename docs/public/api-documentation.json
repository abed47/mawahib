                      "application/json": {
                          "type": "object",
                          "schema": {
                              "type": "object",
                              "properties": {
                                  "email": {"type": "string", "required": true},
                                  "password": {"type": "string", "required": true},
                                  "first_name": {"type": "string"},
                                  "last_name": {"type": "string"},
                                  "dob": {"type": "string", "format": "MM-DD-YYYY"},
                                  "username": {"type": "string"}
                              }
                          }
                      }
                  }
              },
              "responses": {
                  "200": {
                      "description": "operation successful",
                      "content": {
                          "application/json": {
                              "type": "object",
                              "schema": {
                                  "type": "object",
                                  "properties": {
                                      "status": {"type":"boolean"},
                                      "type": {"type": "string"},
                                      "data": {"type": "object"},
